{
  "schema_version": 1,

  "quilt_loader": {
    "group": "${mod_group}",
    "id": "${mod_id}",
    "version": "${mod_version}",

    "intermediate_mappings":"net.fabricmc:intermediary",

    "metadata": {
      "name": "${mod_name}",
      "description": "adds command predictions",
      "contributors": {
        "CrabKing": "Owner"
      },

      "contact": {
        "homepage": "https://example.com/",
        "issues": "https://codeberg.org/CRModders/cosmic-quilt-example/issues",
        "sources": "https://codeberg.org/CRModders/cosmic-quilt-example/"
      },

      "icon": "assets/example_mod/icon.png"
    },

    "entrypoints": {
      "init": "io.github.CrabK1ng.FuckMyDyslexia.FuckMyDyslexia"
    },

    "depends": [
      {
        "id": "cosmicquilt",
        "versions": ">=2.3.1"
      },
      {
        "id": "cosmicreach",
        "versions": ">=0.3.4"
      }
    ]
  },

  "access_widener": "fuck_my_dyslexia.accesswidener",
  "mixin": "fuck_my_dyslexia.mixins.json"
}
